cd ipcmm

python3 -m venv venv  ## -p --python is not necessary just use version

source venv/bin/activate

python -m pip install -U reactor -t ipcmm/lib/
python -m pip install -U twisted -t ipcmm/lib/
python -m pip install -U tornodo -t ipcmm/lib/
python -m pip install -U psutil -t ipcmm/lib/
python -m pip install -U nuitka -t ipcmm/lib/
python -m pip install -U pointers.py -t ipcmm/lib/
python -m pip install -U eventlet -t /appl/ipcmm/lib/
python -m pip install -U greenlet -t /appl/ipcmm/lib/
python -m pip install -U asyncio -t /appl/ipcmm/lib/
python -m pip install -U coroutines -t /appl/ipcmm/lib/
python -m pip install -U generators -t /appl/ipcmm/lib/
python -m pip install -U essential-generators -t /appl/ipcmm/lib/
python -m pip install -U stacket -t /appl/ipcmm/lib/

deactivate

git clone https://github.com/saghul/python-fibers
./build_inplace
